<?xml version='1.0' encoding='utf-8'?>
<filter namespace="powerhouse-objects" name="barcode">
  <label>Object Barcode</label>
  <arg>
    <name>value</name>
    <label>Value</label>
    <type>
      <name>string</name>
    </type>
  </arg>
  <aql>xpath(powerhouse:related-objects/object/@barcode) = '$${value}$$' or xpath(powerhouse.object.of.interest:cataloging/barcode) = '$${value}$$'</aql>
  <tag>ui:PASTE_DELIMITER_COMMA</tag>
  <tag>ui:PASTE_DELIMITER_SPACE</tag>
  <tag>ui:PASTE_DELIMITER_NEW_LINE</tag>
</filter>
